[user]
  name = Ryan Bone
  email = rbone@covermymeds.com
[push]
  default = simple
[core]
  excludesfile = ~/.gitignore
[alias]
  co = checkout
	ctags = !.git/hooks/ctags
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[init]
	templatedir = ~/.git_template
